@font-face {
  font-family: "Flaticon";
  src: url('{{ "Flaticon.eot" | asset_url }}');
  src: url('{{ "Flaticon.eot" | asset_url }}?#iefix') format("embedded-opentype"),
       url('{{ "Flaticon.woff2" | asset_url }}') format("woff2"),
       url('{{ "Flaticon.woff" | asset_url }}') format("woff"),
       url('{{ "Flaticon.ttf" | asset_url }}') format("truetype"),
       url('{{ "Flaticon.svg" | asset_url }}#Flaticon') format("svg");
  font-weight: normal;
  font-style: normal;
  font-display: swap;
}

@media screen and (-webkit-min-device-pixel-ratio:0) {
  @font-face {
    font-family: "Flaticon";
    src: url('{{ "Flaticon.svg" | asset_url }}#Flaticon') format("svg");
  }
}


[class^="flaticon-"]:before, [class*=" flaticon-"]:before {
  font-family: "Flaticon";  /* <--- THIS is critical */
  font-style: normal;
  font-weight: normal;
  speak: never;
  display: inline-block;
  text-decoration: inherit;
  width: 1em;
  margin-right: .2em;
  text-align: center;
  font-variant: normal;
  text-transform: none;
  line-height: 1em;
}
[class^="flaticon-"]:after, [class*=" flaticon-"]:after {   
font-family: Flaticon;
font-size: 20px;
font-style: normal;
margin-left: 20px;
}

